package api.util;

import java.util.Date;

//DTO클래스,VO클래스,도메인,엔티티
//DTO패턴은 디비에 저장되어 있는 데이터를 오브젝트로 변환해서 작업
//계중간에 데이터를 전송
public class StudentDTO {
	private int num;
	private String name;
	private int kor;
	private int math;
	private int eng;
	private Date regdate;
	public StudentDTO() {
		
	}

	public StudentDTO(int num, String name, int kor, int math, int eng) {
		super();
		this.num = num;
		this.name = name;
		this.kor = kor;
		this.math = math;
		this.eng = eng;
		this.regdate = new Date();
	}

	public Integer getNum() {
		// TODO Auto-generated method stub
		return null;
	}
	
}

