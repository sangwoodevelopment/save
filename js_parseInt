<body>
	<script type="text/javascript">
	//숫자모양을 하고 있는 문자열을 숫자로 변경하기
		let num1 = parseInt( prompt("숫자1"));
		let num2 = parseInt( prompt("숫자2"));
		
		alert(parseInt(num1+num2));
		alert(num1/num2);
		
		switch(num1){
			case 1:
				alert("일");
				break;
			case 2:
				alert("이");
				break;	
		}
		
	</script>

