select * from emp where ename like '%S';
select empno, ename, job, sal, deptno from emp where deptno = 30 and job = 'SALESMAN';
select * from emp where sal < 2000 or sal > 3000;
select empno, ename, sal, deptno from emp where ename like '%E%' and deptno = 30 and sal not between 10
00 and 2000;
select * from emp where comm is null and mgr is not null and (job = 'MANAGER' or job = 'CLERK') and ena
me not like '_L%';
